<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="BiasCropImageView">
        <attr name="horizontalCropBias" format="float" />
        <attr name="verticalCropBias" format="float" />
    </declare-styleable>

    <declare-styleable name="DrawView">
        <attr name="drawStrokeColor" format="color" />
        <attr name="drawStrokeWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="SwipeToDeleteView">
        <attr name="swipeToDeleteText" format="string" />
        <attr name="deleteActionText" format="string" />
        <attr name="swipeThumbBackground" format="reference" />
        <attr name="swipeThumbIcon" format="reference" />
        <attr name="swipeThumbIconTint" format="color" />
        <attr name="labelTextColor" format="color" />
    </declare-styleable>

    <declare-styleable name="ReadonlyTextInputView">
        <attr name="iconEmptyText" format="reference" />
        <attr name="iconFilledText" format="reference" />
        <attr name="android:text" />
    </declare-styleable>

    <declare-styleable name="ViewPagerDotView">
        <attr name="dotColorInactive" format="color" />
        <attr name="dotColorActive" format="color" />
        <attr name="dotSize" format="dimension" />
        <attr name="dotGap" format="dimension" />
    </declare-styleable>

</resources>